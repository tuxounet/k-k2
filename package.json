  "bin": "./dist/index.js",
  "files": [
    "dist/",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "start": "node .",
    "prenuke": "npm run clean",
    "nuke": "rm -rf ./node_modules ./package-lock.json",
    "clean": "rm -rf ./dist ./out ./coverage",
    "build": "rollup -c",
    "test": "jest --coverage",
    "presample": "npm run build",
    "samples:clean": "npm run build && npm start ./samples/k2.inventory.yaml clean",
    "samples:apply": "npm run build && npm start ./samples/k2.inventory.yaml apply",
    "samples:list": "npm run build && npm start ./samples/k2.inventory.yaml list"
  },
  "repository": {
    "type": "git",
