{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "@tuxounet-k2/cli",
  "version": "0.0.23",
  "description": "K2 Build System CLI",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": "./dist/bin.js",
  "files": [
    "create/",
    "dist/",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "start": "node .",
    "prenuke": "npm run clean",
    "nuke": "rm -rf ./node_modules ./package-lock.json",
    "clean": "rm -rf ./dist ./out ./init ./coverage",
    "build": "rollup -c",
    "build:watch": "rollup -c -w",
    "dev": "nodemon",
    "lint": "eslint ./src --fix",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:watch-coverage": "jest --watch --coverage",
    "presample": "npm run build",
    "samples:clean": "npm run build && node . clean --inventory ./samples/k2.inventory.yaml",
    "samples:apply": "npm run build && node . apply --inventory ./samples/k2.inventory.yaml",
    "samples:list": "npm run build && node .  list --inventory ./samples/k2.inventory.yaml",
    "samples:create": "npm run build && mkdir -p ./init && cd ./init && node .. create",
    "samples:help": "npm run build && node . --help"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tuxounet/k2.git"
  },
  "keywords": [],
  "author": {
    "email": "krux@krux.fr",
    "name": "Christophe Tiraoui",
    "url": "https://github.com/tuxounet/k2"
  },
  "license": "gplv3",
  "bugs": {
    "url": "https://github.com/tuxounet/k2/issues"
  },
  "homepage": "https://github.com/tuxounet/k2#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^23.0.2",
    "@rollup/plugin-json": "^5.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-typescript": "^9.0.2",
    "@types/ejs": "^3.1.1",
    "@types/jest": "^29.2.6",
    "@types/js-yaml": "^4.0.5",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "commander": "^10.0.0",
    "ejs": "^3.1.8",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.3.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.1",
    "fast-glob": "^3.2.12",
    "jest": "^29.3.1",
    "js-yaml": "^4.1.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "rollup": "^3.2.3",
    "rollup-plugin-dts": "^5.2.0",
    "rollup-plugin-preserve-shebang": "^1.0.1",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
}